var num1 = 5;
var num2 = 6;

console.log(num1 * num2);

var answer = ( num1 < num2);

console.log(answer);

var num1 = 6;
var num2 = 4;

var answer = num1 > num2;
console.log(answer);


var a = 2;
var b = 4;
var c = 6;
var d = 8;
var e = 10;
var answer = a + b * c / d * e;
console.log("The Final Calculated Output'by ordinary method':" + answer);
//>>
var answer = a + ((b*c)/d)*e;
console.log("The Final Calculated Output 'by clean code practice is':" + answer)
 




