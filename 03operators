//one more better way to do the same job is 
var listing_price = 1000;
var selling_price = 759;
var discount_percentage = ((listing_price - selling_price) / listing_price) * 100;
console.log(discount_percentage + "%OFF");

//discount percent (with  round off figure).
var listing_price = 1000;
var selling_price = 759;
var discount_percentage = ((listing_price - selling_price) / listing_price) * 100;
var display_discount_percentage = Math.round(discount_percentage);
console.log(display_discount_percentage + "% OFF");


// Discount
var original_price = 599;
var discount_percent = 29;
var final_price = (original_price - discount_percent*original_price / 100);
var display_final_price = Math.round(final_price);
console.log("The final Price is:" +display_final_price );




//simple intrest 
var principle_amount = 10000;
var time = 2; 
var rate_of_intrest = 4;

var interst  = ((principle_amount * time *rate_of_intrest ) / 100);
console.log("The simple intrest is:"+interst);


var principle_amount = 10000;
var time = 2;
var rate_of_interest = 4;

var interest = (principle_amount * time * rate_of_interest) / 100;
console.log("The simple interest is: " + interest);


var principle = 5999;
var time = 3;
var rate = 5;
var simple_intrest = (principle * time * rate) / 100;
var display_simple_intrest = Math.round(simple_intrest);
console.log(display_simple_intrest);

//area of circle.
var side = 5;
var area = (side * side)
var display_area = Math.round(area)
console.log("\nThe area of circle is:" + display_area);


//area of rectangle.

var length = 5;
var breadth = 7;
var area = (length * breadth);
console.log("\n\nThe area of rectangle is:" + area);

//area of triangle.
var base = 5;
var height = 79.545;
var area = (0.5 * base * height);
var display_area = Math.round(area);
console.log("\nThe area of triangle is:" + display_area);

//area  of  circle.

var radius = 5;
var area = (3.14 * radius * radius);
var display_area = Math.round(area);
console.log("\nThe area of circle is:" + display_area);

